<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>History</servlet-name>
  	<display-name>History</display-name>
  	<description></description>
  	<servlet-class>com.controller.History</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>manageApi</servlet-name>
  	<display-name>manageApi</display-name>
  	<description></description>
  	<servlet-class>com.api.manageApi</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>History</servlet-name>
  	<url-pattern>/History</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>manageApi</servlet-name>
  	<url-pattern>/manageApi</url-pattern>
  </servlet-mapping>
</web-app>
